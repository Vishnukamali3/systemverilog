class parent;
  local int a=10;
  local int b=7;
  function void print();
    $display("a=%d b=%d",a,b);
  endfunction
endclass

class child extends parent;
  int c;
  function void sub();
    c=a-b;
    $display("c=%d",c);
  endfunction
endclass

module tb;
  child ch;
  initial begin
    ch=new();
    ch.print();
    ch.sub();
  end
endmodule



ERROR VCP5248 "Cannot access local/protected member ""a"" from this scope." "testbench.sv" 14  8
ERROR VCP5248 "Cannot access local/protected member ""b"" from this scope." "testbench.sv" 14  10
