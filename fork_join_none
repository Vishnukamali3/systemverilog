module tb;
  task one();
    $display("task1 is started=%t",$time);
    #20;
    $display("task1 is completed=%t",$time);
  endtask
  task two();
    $display("task2 is started=%t",$time);
    #30;
    $display("task2 is completed=%t",$time);
  endtask
  task three();
    $display("task3 is started=%t",$time);
    #10;
    $display("task3 is completed=%t",$time);
  endtask
  initial
    begin
      fork
        one();
        two();
      join_none
      three();
    end
endmodule
