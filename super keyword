class parent;
  int a=10;
  function void display();
    $display("a=%d",a);
  endfunction
endclass

class child extends parent;
  int a=15;
  function void display();
    super.a=20;
    super.display();
    $display("a=%d",a);
  endfunction
endclass

module tb;
  child c;
  initial begin
    c=new();
    c.display();
  end
endmodule



a=         20
# KERNEL: a=         15
