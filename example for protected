class parent;
  protected int a=10;
  protected int b=7;
  function void print();
    $display("a=%d b=%d",a,b);
  endfunction
endclass

class child extends parent;
  int c;
  function void sub();
    c=a-b;
    $display("c=%d",c);
  endfunction
endclass

module tb;
  child ch;
  int d;
  initial begin
    ch=new();
    ch.print();
    ch.sub();
    d=ch.a+ch.b;
    $display("D=%d",d);
  end
endmodule



ERROR VCP5248 "Cannot access local/protected member ""ch.a"" from this scope." "testbench.sv" 26  11
ERROR VCP5248 "Cannot access local/protected member ""ch.b"" from this scope." "testbench.sv" 26  16
